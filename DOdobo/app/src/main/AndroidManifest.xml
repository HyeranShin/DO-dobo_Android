<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hyeran.android.dodobo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.InformDoboActivity" />
        <activity android:name=".home.InformSeoulightActivity" />
        <activity android:name=".mypage.SuggestionActivity" />
        <activity android:name=".mypage.ModifyActivity" />
        <activity android:name=".mypage.ApplySeoulightActivity"></activity>
        <activity android:name=".mypage.MypageTab"></activity>
    </application>

</manifest>